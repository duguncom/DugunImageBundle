{
  "name": "duguncom/imagebundle",
  "description": "An image manipulation bundle for duguncom and anneysencom",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "salah aldin alsasa",
      "email": "salahalsasa@gmail.com"
    },
    {
      "name": "guven atbakan",
      "email": "guven@atbakan.com"
    }
  ],
  "keywords": [
    "image bundle"
  ],
  "license": [
    "MIT"
  ],
  "config": {
    "platform": {
      "ext-gd": "dev-master"
    }
  },
  "require": {
    "php": "^7.1.3",
    "symfony/http-kernel": "~4.0",
    "intervention/image": "^2.3",
    "symfony/yaml": "^4.0"
  },
  "require-dev": {
    "symfony/framework-bundle": "^4.0.8",
    "phpunit/phpunit": "^6.5"
  },
  "autoload": {
    "psr-0": {
      "Dugun\\ImageBundle": ""
    }
  },
  "target-dir": "Dugun/ImageBundle"
}
